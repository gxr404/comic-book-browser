{
  "name": "comic-book-browser",
  "version": "1.0.3",
  "description": "Comic Book Browser ",
  "bin": {
    "comic-book-browser": "bin/index.js"
  },
  "main": "index.js",
  "keywords": [
    "manga",
    "comic",
    "nodejs",
    "download",
    "comic-browser",
    "comic-book-browser",
    "cli"
  ],
  "author": "gxr404",
  "license": "ISC",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gxr404/comic-book-browser.git"
  },
  "scripts": {
    "dev:client": "vite client",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "dev:server": "tsc -p server/tsconfig.json && (concurrently \"tsc -p server/tsconfig.json -w\" \"tsc-alias -p server/tsconfig.json -w\")",
    "build": "run-s build:*",
    "build:client": "tsc -p ./client/tsconfig.json && vite build client",
    "build:server": "tsc -p server/tsconfig.json && tsc-alias -p server/tsconfig.json",
    "eslintLog": "eslint . > eslint.log",
    "clean": "rimraf client-dist server-dist",
    "np": "np --no-tests",
    "release": "run-s clean build np"
  },
  "dependencies": {
    "@koa/router": "^12.0.1",
    "cac": "^6.7.14",
    "koa": "^2.15.0",
    "koa-bodyparser": "^4.4.1",
    "koa-mount": "^4.0.0",
    "koa-static": "^5.0.0",
    "log4js": "^6.9.1",
    "p-limit": "^5.0.0"
  },
  "devDependencies": {
    "@types/koa": "^2.13.12",
    "@types/koa__router": "^12.0.4",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa-mount": "^4.0.5",
    "@types/koa-static": "^4.0.4",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "ahooks": "^3.7.8",
    "antd": "^5.12.7",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "np": "^9.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.33",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1",
    "react-virtuoso": "^4.6.2",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.0",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "np": {
    "tests": false,
    "2fa": false
  }
}
